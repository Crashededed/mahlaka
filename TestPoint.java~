class TestPoint
{

  
  public static void main(String[] args)
  {
    
   Point a=new Point(2,6);
   System.out.println(a);
   Point b=new Point(2,6);
   System.out.println(b);
   boolean e=a.equal(b);
   if(e)
    System.out.println("they are equal");
  else 
    System.out.println("they are unequal");
    
  

  
  
  }
}